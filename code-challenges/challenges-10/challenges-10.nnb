            "source": [
                "function migratoryBirds(arr) {\n    // Write your code here\n    let result = []\n    let typ1=0; let typ2=0; let typ3 =0; let typ4=0; let typ5=0;\n    for (let i = 0; i < arr.length; i++) \n    {\n      if(arr[i]>=1 && arr[i]<=5)\n      {\n        if(arr[i]==1)\n        {\n          typ1++;\n          \n        }\n        else if(arr[i]==2)\n        {\n          typ2++;\n          \n        }\n        else if(arr[i]==3)\n        {\n          typ3++;\n          \n        }\n        else if(arr[i]==4)\n        {\n          typ4++;\n         \n        }\n        else if(arr[i]==5)\n        {\n          typ5++;\n        }\n\n      }\n      else\n      {\n        console.log(\"Invalid\")\n      }\n      \n    }\n    \n    result.push(typ1,typ2,typ3,typ4,typ5);\nlet n;\nlet x =Math.max(...result);\n    for (var i = 0; i < result.length; i++) {\n          if(result[i]== x)\n          {\n            n =i+1;\n            break;\n          }\n\n    }\n    return n;\n}\n"
